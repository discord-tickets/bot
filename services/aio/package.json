{
	"name": "@discord-tickets/aio",
	"version": "0.0.1",
	"type": "module",
	"module": "src/",
	"private": true,
	"scripts": {
		//  --bytecode can halve startup time but at up to 8x size and is currently CJS only
		"build": "bun build --compile --minify --sourcemap --outfile aio src ../interceptor/src",
		"dev": "bun --hot src"
	},
	"devDependencies": {
		"@types/bun": "catalog:"
	},
	"peerDependencies": {
		"typescript": "catalog:"
	},
	"dependencies": {
		"@discord-tickets/logger": "workspace:*"
	}
}